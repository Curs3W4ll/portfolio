{
  "name": "portfolio-corentin-hervaud",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "build:preview": "nuxt preview",
    "dev": "NODE_TLS_REJECT_UNAUTHORIZED=0 nuxt dev --https --ssl-cert localhost.pem --ssl-key localhost-key.pem",
    "generate": "nuxt generate",
    "generate:preview": "NODE_TLS_REJECT_UNAUTHORIZED=0 serve .output/public --ssl-cert localhost.pem --ssl-key localhost-key.pem",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test:unit": "vitest run",
    "test:unit:dev": "vitest --ui",
    "test:e2e": "start-server-and-test 'PORT=4173 nuxt preview' http://localhost:4173 'cypress run --e2e'",
    "test:e2e:record": "start-server-and-test 'PORT=4173 nuxt preview' http://localhost:4173 'cypress run --e2e --record'",
    "test:e2e:dev": "start-server-and-test dev http://localhost:4173 'cypress open --e2e' --browser chrome",
    "storyblok:login": "storyblok login",
    "storyblok:updatetypes": "storyblok pull-components --space 314995 && storyblok generate-typescript-typedefs --sourceFilePaths ./components.314995.json --destinationFilePath ./components-types-sb.d.ts",
    "postinstall": "nuxt prepare",
    "prepare": "husky"
  },
  "dependencies": {
    "@nuxt/devtools": "^1.6.1",
    "@nuxtjs/robots": "^5.0.0",
    "@nuxtjs/sitemap": "^7.0.0",
    "@pinia/nuxt": "^0.7.0",
    "@primevue/nuxt-module": "^4.2.3",
    "@primevue/themes": "^4.2.3",
    "@sentry/nuxt": "^8.40.0",
    "@storyblok/nuxt": "^6.2.0",
    "nuxt": "^3.14.4",
    "nuxt-gtag": "^3.0.2",
    "primeicons": "^7.0.0",
    "primevue": "^4.2.3",
    "tailwindcss-primeui": "^0.3.4",
    "vue": "^3.5.13",
    "vue-router": "^4.3.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@nuxt/eslint": "^0.7.0",
    "@nuxt/test-utils": "^3.14.4",
    "@types/eslint__eslintrc": "^2.1.1",
    "@vitest/ui": "^2.1.5",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.6.0",
    "autoprefixer": "^10.4.20",
    "cypress": "^13.10.0",
    "eslint-plugin-cypress": "^4.1.0",
    "happy-dom": "^15.11.6",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.7",
    "postcss": "^8.4.49",
    "prettier": "^3.4.1",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "sass": "^1.81.0",
    "serve": "^14.2.4",
    "start-server-and-test": "^2.0.3",
    "storyblok": "^3.35.1",
    "tailwindcss": "^3.4.15",
    "typescript": "5.6.2",
    "vite-plugin-eslint2": "^5.0.2",
    "vitest": "^2.1.5",
    "vue-tsc": "^2.1.10"
  },
  "lint-staged": {
    "*": [
      "npm run format"
    ],
    "**/*.{js,cjs,mjs,ts,cts,mts,vue}": [
      "npm run lint:fix"
    ]
  }
}
